<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.unirail">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/esicon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Channel.Channel_post"></activity>
        <activity android:name=".Channel.Channel_get" />
        <activity android:name=".Channel.Channel_main" />

        <service
            android:name=".Navigator.Navigator"
            android:enabled="true"
            android:exported="true" />

        <activity
            android:name=".SplashActivity"
            android:screenOrientation="portrait"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" /> <!-- <activity android:name=".TrafficSubwayInfo" /> -->
        <activity android:name=".StartSearchActivity" />
        <activity android:name=".FinalSearchActivity" />
        <activity android:name=".SettingActivity" />
        <activity android:name=".searchRoute" />
        <activity android:name=".SearchRouteResult" />
        <activity android:name=".RecentRoute" />
        <activity android:name=".SearchMap" />
        <activity android:name=".StationMapActivity" />
    </application>

</manifest>